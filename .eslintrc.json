{
    "env": {
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "arrow-body-style": "error",
        "arrow-spacing": "error",
        "eol-last": "error",
        "camelcase": "error",
        "indent": ["error", 2],
        "strict": "error"
    },
    "extends": [
        "prettier"
    ]
}
